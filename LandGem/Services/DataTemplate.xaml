<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:LandGEM.ViewModels"
                    xmlns:view="clr-namespace:LandGEM.Views">

    <DataTemplate DataType="{x:Type vm:PrimaryContorolViewModel}">
        <view:PrimaryControlWindow />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DataEntryViewModel}">
        <view:DataInsertionPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DataReviewViewModel}">
        <view:DataReviewPage />
    </DataTemplate>

</ResourceDictionary>
