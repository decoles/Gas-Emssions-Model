<Window x:Class="LandGEM.Views.PrimaryControlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LandGEM.Views"
        mc:Ignorable="d"
        Title="PrimaryControlWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="1" Content="Next" Width="50" Height="30" HorizontalAlignment="Right"/>
        <Button Grid.Row="1" Content="Back" Width="50" Height="30" HorizontalAlignment="Center"/>


    </Grid>
</Window>
